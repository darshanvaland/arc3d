

# Enable rewriting
<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Force HTTPS and WWW (optional — comment out if not needed)
    <!-- RewriteCond %{HTTPS} !=on [OR] -->
    <!-- RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
    RewriteRule ^ https://%1%{REQUEST_URI} [L,R=301] -->
    
    # redirect as per satish sir seo
    RewriteRule ^cookies?$ https://arc3d.ae/ [R=301,L]
    RewriteRule ^project-gallery?$ https://arc3d.ae/our-projects [R=301,L]    
   
    RewriteRule ^pioneering-progress-3d-model-making-dubai-uae?$ https://arc3d.ae/blogs  [R=301,L]
    RewriteRule ^better-1-deposit-gambling-enterprises-within-the-13?$ https://arc3d.ae/blogs  [R=301,L]
    RewriteRule ^the-most-effective-online-online-casinos-that-accept-bitcoin?$ https://arc3d.ae/blogs  [R=301,L]
    RewriteRule ^better-1-deposit-gambling-enterprises-within-the-13-2?$ https://arc3d.ae/blogs  [R=301,L]
    RewriteRule ^5-ways-3d-printing-companies-in-dubai-abu-dhabi-are-innovating-the-business-landscape?$ https://arc3d.ae/blogs  [R=301,L]
    RewriteRule ^from-traditional-craftsmanship-to-3d-printing-in-the-uae-the-evolution-of-architectural-maquettes?$ https://arc3d.ae/blogs  [R=301,L]
    
    RewriteRule ^services-detail/3d-printing?$ https://arc3d.ae/3d-printing  [R=301,L]
    RewriteRule ^3d-printing/3d-printing-services-in-abu-dhabi?$ https://arc3d.ae/3d-printing  [R=301,L]
    
    # Handle Authorization Header
    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    # Redirect Trailing Slashes If Not A Folder
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_URI} (.+)/$
    RewriteRule ^ %1 [L,R=301]

    # Send all other requests to index.php
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]
</IfModule>

